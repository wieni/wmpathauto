{
    "name": "wieni/wmpathauto",
    "type": "drupal-module",
    "description": "Dynamic path aliases, pathauto patterns & automatic updating of aliases",
    "license": "MIT",
    "authors": [
        {
            "name": "Wieni",
            "email": "info@wieni.be"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "drupal/core": "^9.4 || ^10.0 || ^11.0",
        "drupal/pathauto": "^1.6"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.0",
        "wieni/wmcodestyle": "^1.3"
    },
    "repositories": {
        "drupal-composer": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "scripts": {
        "coding-standards": [
            "@composer normalize",
            "php-cs-fixer fix --config=.php_cs.php"
        ]
    }
}
